@model IEnumerable<Account>
@{
    ViewData["Title"] = "Home Page";
}



<h1>Item List</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.Password</td>
                <td>@item.CodeAccount</td>
            </tr>
        }
    </tbody>
</table>
